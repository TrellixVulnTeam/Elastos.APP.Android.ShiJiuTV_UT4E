rm -r src/out
rm -f chromium.gyp_env compile_command_sample gclient-revinfo
find ./ -name .git |xargs rm -r
find ./ -name .svn |xargs rm -r
cat .gclient_entries |grep src |sed "s/'\(src.*\)':.*/\1/g" |xargs git add --force --verbose

git init
git add * .gclient*
git rm -f *.pyc
git commit -m "cr59-beta(59.0.3071.63) init."
git gc
rm -r src && git reset --hard HEAD

export PATH=$PATH:$PWD/depot_tools;
cd src
gn gen --args='target_os="android"' out/Default
cp ../qc-utils/args.gn out/Default/
ninja -C out/Default android_webview_apk -j8
